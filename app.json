{
  "expo": {
    "name": "Expo Template",
    "slug": "expo-template",
    "scheme": "com.expotemplate",
    "owner": "nickderaj",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/app/icon.png",
    "userInterfaceStyle": "light",
    "backgroundColor": "#f5f5f5",
    "splash": {
      "image": "./assets/images/app/splash.png",
      "backgroundColor": "#f5f5f5",
      "resizeMode": "cover"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.expotemplate",
      "usesAppleSignIn": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "buildNumber": "1"
    },
    "android": {
      "icon": "./assets/images/app/icon.png",
      "package": "com.expotemplate",
      "googleServicesFile": "./google-services.json",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/images/app/icon.png"
    },
    "androidNavigationBar": {
      "backgroundColor": "#171717"
    },
    "extra": {
      "eas": {
        "projectId": "2b7d47ec-3fcb-4b7d-a52d-0c33a3997dab"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/images/app/icon.png",
          "color": "#171717"
        }
      ]
    ]
  }
}
